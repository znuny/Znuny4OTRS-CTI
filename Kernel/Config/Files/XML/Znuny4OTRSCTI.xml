<?xml version="1.0" encoding="utf-8" ?>
<!-- nofilter(TidyAll::Plugin::OTRS::Znuny4OTRS::Config::ACLKeysLevel3Actions) -->
<otrs_config version="2.0" init="Application">
    <Setting Name="Frontend::Module###AgentTicketPhoneCTI" Required="0" Valid="1">
        <Description Translatable="1">Frontend module registration for the agent interface. E. g. 'otrs/index.pl?Action=AgentTicketPhoneCTI;CallerID=XXX;MSN=XXX'.</Description>
        <Navigation>Frontend::Agent::ModuleRegistration</Navigation>
        <Value>
            <Item ValueType="FrontendRegistration">
                <Hash>
                    <Item Key="Group">
                        <Array>
                        </Array>
                    </Item>
                    <Item Key="GroupRo">
                        <Array>
                        </Array>
                    </Item>
                    <Item Key="Description">CTI</Item>
                    <Item Key="Title"></Item>
                    <Item Key="NavBarName"></Item>
                </Hash>
            </Item>
        </Value>
    </Setting>
    <Setting Name="CTI::MSN::Action::Map" Required="0" Valid="1">
        <Description Translatable="1">Expert Setting - not needed in standard setup! Defines the Action based on MSN, e. g. route incoming calls to different frontend modules (Key=Number,Value=Frontend-Module e. g. Key=1222,Value=AgentTicketPhoneHotline).</Description>
        <Navigation>Core::Znuny4OTRS::CTI</Navigation>
            <Value>
                <Hash>
                    <Item Key="">AgentTicketPhone</Item>
                </Hash>
            </Value>
    </Setting>
    <Setting Name="CTI::MaxUsers" Required="0" Valid="0">
        <Description Translatable="1">This configuration defines the maximum of CustomerUsers that should get added to the the AgentTicketPhone view.</Description>
        <Navigation>Core::Znuny4OTRS::CTI</Navigation>
        <Value>
            <Item ValueType="String" ValueRegex="^\d+$">1</Item>
        </Value>
    </Setting>
    <Setting Name="CTI::CallerIDRegEx" Required="0" Valid="1">
        <Description Translatable="1">Regular expression for the CallerID.</Description>
        <Navigation>Core::Znuny4OTRS::CTI</Navigation>
        <Value>
            <Item ValueType="String" ValueRegex="">^.+$</Item>
        </Value>
    </Setting>
    <Setting Name="CTI::FilterCallerRegEx" Required="0" Valid="0">
        <Description Translatable="1">This configuration defines a list of replacement regexes which will be used to filter the caller id.</Description>
        <Navigation>Core::Znuny4OTRS::CTI</Navigation>
        <Value>
            <Hash>
                <Item Key="[\(\)]"></Item>
                <Item Key="\-"></Item>
                <Item Key="\s*"></Item>
                <Item Key="\+">*</Item>
            </Hash>
        </Value>
    </Setting>
</otrs_config>
